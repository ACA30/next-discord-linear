datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["selectRelationCount"]
}

model Endpoint {
  id String @id // set by user/auto generated by client

  enabled Boolean @default(true)

  hookId    String
  hookToken String
}
